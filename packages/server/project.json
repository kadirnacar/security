{
  "root": "packages/server",
  "sourceRoot": "packages/server/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/server",
        "main": "packages/server/src/main.ts",
        "tsConfig": "packages/server/tsconfig.app.json",
        "assets": [
          "packages/server/src/main.js",
          "packages/server/src/model",
          "packages/server/src/rtspgo",
          "packages/server/src/assets",
          "packages/server/src/alpr",
          "packages/server/src/data"
        ],
        "webpackConfig": "packages/server/webpack.config.js"
      },
      "configurations": {
        "production": {
          "webpackConfig": "packages/server/webpack.config.prod.js",
          "optimization": true,
          "inspect": false,
          "sourceMap": false,
          "vendorChunk": true,
          "subresourceIntegrity": true,
          "buildLibsFromSource": true,
          "generatePackageJson": true,
          "fileReplacements": [
            {
              "replace": "packages/server/src/environments/environment.ts",
              "with": "packages/server/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "server:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/server/**/*.ts"]
      }
    }
  },
  "tags": []
}
