{
  "root": "packages/viewer",
  "sourceRoot": "packages/viewer/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "compiler": "babel",
        "outputPath": "dist/packages/viewer",
        "index": "packages/viewer/src/index.html",
        "baseHref": "/",
        "main": "packages/viewer/src/main.tsx",
        "polyfills": "packages/viewer/src/polyfills.ts",
        "tsConfig": "packages/viewer/tsconfig.app.json",
        "assets": [
          "packages/viewer/src/favicon.ico",
          "packages/viewer/src/assets"
        ],
        "styles": ["packages/viewer/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "packages/viewer/src/environments/environment.ts",
              "with": "packages/viewer/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": true
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "webpackConfig":"",
      "options": {
        "buildTarget": "viewer:build",
        "proxyConfig": "packages/viewer/proxy.conf.json",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "viewer:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/viewer/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
